{
  "name": "xmtp-agent-alphie",
  "version": "1.0.0",
  "exports": "./dist/index.js",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "viem": "2.37.8"
    }
  },
  "resolutions": {
    "viem": "2.37.8"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write .",
    "format": "npx @biomejs/biome format --write ./src",
    "gen:keys": "pnpm build && node ./dist/utils/generate-keys.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:pull": "drizzle-kit pull",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@ai-sdk/openai": "2.1.0-beta.8",
    "@coinbase/onchainkit": "^1.1.0",
    "@libsql/client": "^0.15.15",
    "@t3-oss/env-core": "^0.13.8",
    "@xmtp/agent-sdk": "^1.0.1",
    "@xmtp/content-type-group-updated": "^2.0.2",
    "@xmtp/content-type-primitives": "^2.0.2",
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-remote-attachment": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "ai": "5.1.0-beta.14",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "ky": "^1.11.0",
    "uint8arrays": "^5.1.0",
    "ulid": "^3.0.1",
    "viem": "^2.37.9",
    "zod": "^4.1.11",
    "zod-validation-error": "^4.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.5.2",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "ultracite": "^5.4.5"
  }
}
